{
  "name": "@justifi/webcomponents",
  "version": "3.0.1",
  "description": "JustiFi Web Components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justifi-tech/web-component-library.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "auto-changelog": "auto-changelog",
    "release": "release-it --no-npm",
    "build": "stencil build --docs",
    "start": "concurrently -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"yarn run start-stencil\" \"yarn run storybook\"",
    "start-stencil": "stencil build --watch --serve --no-open",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006 --quiet",
    "build-storybook": "storybook build -o ../docs"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0",
    "@stencil/react-output-target": "^0.3.1",
    "bootstrap": "^5.2.3",
    "date-fns": "^2.29.3",
    "dinero.js": "^1.9.1",
    "uuid": "^9.0.0",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil/sass": "^3.0.1",
    "@storybook/addon-actions": "^7.0.4",
    "@storybook/addon-essentials": "^7.0.4",
    "@storybook/addon-interactions": "^7.0.4",
    "@storybook/addon-links": "^7.0.4",
    "@storybook/html": "^7.0.4",
    "@storybook/html-webpack5": "^7.0.4",
    "@storybook/jest": "^0.1.0",
    "@storybook/testing-library": "^0.1.0",
    "@types/jest": "27.0.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "auto-changelog": "^2.4.0",
    "concurrently": "^7.6.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^15.6.0",
    "storybook": "^7.0.4"
  },
  "license": "MIT"
}