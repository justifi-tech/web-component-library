{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "build:mocks": {
      "outputs": [
        "dist/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "webcomponents#build"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "test:watch": {
      "dependsOn": [
        "^build"
      ]
    },
    "test:e2e": {},
    "test:file": {},
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "webcomponents#build"
      ]
    },
    "dev:mocks": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "webcomponents#build",
        "webcomponents#dev"
      ]
    },
    "preview-storybook": {
      "outputs": [
        "dist/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "build"
      ]
    },
    "clean": {
      "cache": false
    }
  }
}
