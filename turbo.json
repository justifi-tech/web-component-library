{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "test:e2e": {},
    "test:file": {},
    "lint": {},
    "preview-storybook": {
      "outputs": [
        "dist/**",
        "storybook-static/**"
      ],
      "dependsOn": [
        "build"
      ]
    },
    "clean": {
      "cache": false
    }
  }
}
