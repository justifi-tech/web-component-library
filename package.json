{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:mocks": "turbo run build:mocks",
    "dev": "concurrently 'pnpm --filter @justifi/webcomponents dev' 'cross-env VITE_STORYBOOK_MOCKS_ENABLED=false pnpm --filter @repo/docs dev'",
    "dev:mocks": "concurrently 'pnpm --filter @justifi/webcomponents dev' 'cross-env VITE_STORYBOOK_MOCKS_ENABLED=true pnpm --filter @repo/docs dev'",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:e2e": "turbo run test:e2e",
    "test:file": "turbo run test:file",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=docs^... && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "prettier": "^3.1.1",
    "turbo": "^1.x.x"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@8.9.0",
  "pnpm": {
    "overrides": {
      "express@<4.19.2": ">=4.19.2"
    }
  }
}